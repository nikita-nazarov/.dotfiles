# Set PATH, MANPATH, etc., for Homebrew.
eval "$(/opt/homebrew/bin/brew shellenv)"

export JDK_17="/Users/nikitanazarov/Library/Java/JavaVirtualMachines/corretto-17.0.5/Contents/Home"
export JDK_1_8="/Users/nikitanazarov/Library/Java/JavaVirtualMachines/corretto-1.8.0_35/Contents/Home"
